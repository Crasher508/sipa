[loggers]
keys=root,sipa

[handlers]
keys=hand01

[formatters]
keys=form01


[logger_root]
level=DEBUG
handlers=hand01

[logger_sipa]
qualname=sipa
handlers=
propagate=1

[handler_hand01]
class=StreamHandler
level=NOTSET
formatter=form01
args=(sys.stdout,)

[formatter_form01]
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s
datefmt=
class=logging.Formatter